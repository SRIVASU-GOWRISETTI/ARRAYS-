   long long maxSubarraySum(int arr[], int n){
        
        // Your code here
        long long sum=0;
        long long max=INT_MIN;
        for(long long i=0;i<n;i++){
            sum+=arr[i];
            if(sum>max){
                max=sum;
            }
            if(sum<0){
                sum=0;
            }
            
            
            
        }
        return max;
        
    }
